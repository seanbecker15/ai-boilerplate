    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tsx": "^4.7.1",
    "turbo": "^2.0.11",
    "typescript": "^5.4.5",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2"
  }
}
