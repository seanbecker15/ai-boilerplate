    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "prisma": "^5.10.2",
    "rimraf": "^5.0.5",
    "tsup": "^8.2.3",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
